<%- include('../partials/head') %>
<body>
  <h1>Skills I Have As A Developer</h1>
  <ul>
    <% skills.forEach(function(skill) { %>
      <li>
        <% if(skill.skill !== 'Can successfully route paths') { %>
          <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
        <% } else { %> 
        <%=  skill.skill %>
        <% } %>
          - 
        <% if(skill.skill === 'Can make Neon CSS') {  %> 
          <span class="neon"><%= skill.aquired %></span>
        <% } else { %>
          <%= skill.aquired %>
        <% } %>
      </li>
    <% }); %>
  </ul>
</body>
</html>