<%- include('../partials/head') %>
<body>
  <h1>Add a New Skill to Aspire to Aquire</h1>
  
  <form action="/skills/<%= skill.id %>?_method=PUT" method="POST" autocomplete="off">
    <input type="text" name="skill" value="&lt;%= skill.name %&gt;">
    <input type="checkbox" name="aquired" <%= skill.aquired ? 'checked' : '' %> >
    <button type="submit">Submit New Skill</button>
  </form>
  
  <a href="/skills">back to skills</a>
</body>
</html>